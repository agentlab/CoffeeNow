   <configurations>
      <plugin id="org.apache.uima.fit.osgi" autoStart="true" startLevel="1" />
      <plugin id="org.apache.uima.fit.osgi.examples" autoStart="false" startLevel="5" />
      <plugin id="org.eclipse.equinox.ds" autoStart="true" startLevel="1" />
      <plugin id="org.eclipse.osgi" autoStart="true" startLevel="-1" />
